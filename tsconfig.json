{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "ng-zorro-antd": [ "./components/ng-zorro-antd.module.ts" ],
      "ng-zorro-antd-ext": [ "./components-ext/yzt-custom.module.ts" ],
      "ngx-color/sketch": [ "./node_modules/ngx-color/sketch" ],
      "@zorro": [ "./components/" ],
      "@zorro/*": [ "./components/*" ],
      "@zorro-ext": [ "./components-ext/" ],
      "@zorro-ext/*": [ "./components-ext/*" ],
      "@env": [ "environments/" ],
      "@env/*": [ "environments/*" ]
    }
  }
}
